ErrorDocument 404 /page/404.html
ErrorDocument 403 /page/403.html

#SymLinks Start
RewriteEngine On 
RewriteRule ^403$ /page/403.html
RewriteRule ^404$ /page/404.html
#SymLinks End

	#Allow FILES
     <Files ~ (LICENSE|README.md)$>
       Order allow,deny
       Allow from all
    </Files>
    #Allow FILES
    <Files ~ (.scss)$>
      Order allow,deny
      deny from all
   </Files>